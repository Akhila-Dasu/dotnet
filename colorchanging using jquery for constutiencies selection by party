    <script>
        $(document).ready(function () {
            $('.totalCount').keyup(function () {
                var count = 0;
                $('.totalCount').each(function () {
                    count += Number($(this).val());
                });
                if (count > 25) {
                    $('#cphBody_labelId').text(count);
                    $('#cphBody_lblerror').text("Oops!! Exceeded maximum number of seats.");
                }
                else if (count <= 25) {
                    $('#cphBody_labelId').text(count);
                    $('#cphBody_lblerror').text("");
                }
            });
            var count = 0;
            $('.totalCount').each(function () {
                count += Number($(this).val());
            });
            if (count > 25) {
                $('#cphBody_labelId').text(count);
                $('#cphBody_lblerror').text("Oops!! Exceeded maximum number of seats.");
            }
            else if (count <= 25) {
                $('#cphBody_labelId').text(count);
                $('#cphBody_lblerror').text("");
            }
        });
    </script>
    
    
