                    tdp.removeClass('tdp');
                    ysr.removeClass('ysrcp');
                    bjp.removeClass('bjp');
                    inc.removeClass('inc');
                    others.removeClass('others');
                    $(this).addClass('janasena');
                    CountofHigh();
